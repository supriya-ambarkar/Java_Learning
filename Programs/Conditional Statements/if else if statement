public class if_else_if
{
    public static void main(String a[])
    {
        int x = 8;
        int y = 9;
        int z = 7;

        if(x>y && x>z)
            System.out.print("X is greater");
        else if(y>z && y>x)
            System.out.print("Y is greater");
        else
            System.out.print("Z is greater");
    }
}
